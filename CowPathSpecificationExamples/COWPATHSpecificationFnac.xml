<scxml version="1.0" xmlns="http://www.w3.org/2005/07/scxml" clock="true" notes="true" previewEnable="false" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<webflow id="checkProduct" initial="fnac">
	  <state id="fnac" URLPattern="*www.fnac.es/*" structuralPattern="//*[@class='nav-diaporama']/span/a/img">
		<transition event="COWPATH:CLICK" cond="['fnac2kelkoo']" target="kelkoo">
		  <script>GoTo("http://www.kelkoo.es/").Paste("$item_name").Into("//*[@name='siteSearchQuery']").Click("//*[@type='submit']");</script>
		</transition>
		<onentry>
		  <script>Copy("//*[@class='titre dispeblock']/text()").Into("$item_name");</script>
		</onentry>
	  </state>
	  <state id="kelkoo" URLPattern="*shopping.kelkoo.es/*">
		<transition event="COWPATH:CLICK" cond="['back2fnac']" target="fnac"/>
	  </state>
	</webflow>
</scxml>


