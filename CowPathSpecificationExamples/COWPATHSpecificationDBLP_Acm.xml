<scxml version="1.0" xmlns="http://www.w3.org/2005/07/scxml" 
	clock="true" notes="true" previewEnable="false" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<webflow id="findArticleACM" initial="authorPublications">
		<state id="authorPublications" URLPattern="http://dblp.uni-trier.de/pers/*">
			<transition event="COWPATH:CLICK" cond="['//*[@class='data']']" target="acmResults">
				<script>
					GoTo("http://dl.acm.org/advsearch.cfm?coll=DL&amp;dl=ACM").
					Script("var title=JSON.stringify(GM_getValue('$title')); GM_setValue('$title',title);").
					Paste("$title").Into("//input[@name='allofem']").
					Click("//input[@name='Go']");
				</script>
			</transition>
			<onentry>
				<script>Copy("span[last()]").Into("$title");</script>
			</onentry>
		</state>
		<state id="acmResults" URLPattern="http://dl.acm.org/results.cfm?*">
			<transition event="COWPATH:CLICK" cond="['back2authorPublications']" target="authorPublications"/>
			<transition event="HOST:CLICK" cond="['//td[2]/table/tbody/tr/td/a[@class='medium-text']']" target="acmArticleInfo"/>
		</state>
		<state id="acmArticleInfo" URLPattern="http://dl.acm.org/citation.cfm?id=*">
			<transition event="COWPATH:CLICK" cond="['back2authorPublications']" target="authorPublications"/>
		</state>
	</webflow>
</scxml>



